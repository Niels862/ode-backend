let alpha = 0.04;

chip {
    io: [
        input,
        -,
        output,
    ],
    cabs: [
        cab 1 with clocks 1, - {
            cams: [
                Integrator as integ1 {
                    integ_const: 1,
                    reset: false
                }
            ]
        },
        cab 2 with clocks 1, - {
            cams: [
                SumInv as sum1 {
                    gain1: alpha,
                    gain2: 2 * alpha
                }
            ]
        }
    ],
    routing: [
        io1 -> integ1,
        integ1 -> io3
    ]
}
